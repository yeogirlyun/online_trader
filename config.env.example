# Online Trader Configuration File
# Copy this file to config.env and fill in your actual values

# Data Configuration
SENTIO_DATA_SUFFIX="_RTH_NH"
SENTIO_DB_PATH="audit/sentio_audit.sqlite3"

# Polygon.io API Key (required for data downloading)
# Get your free API key from: https://polygon.io/
POLYGON_API_KEY="your_api_key_here"

# Alpaca API Keys (optional, for live trading)
ALPACA_API_KEY="your_alpaca_key"
ALPACA_SECRET_KEY="your_alpaca_secret"

# Trading Configuration
DEFAULT_SYMBOL="QQQ"
DEFAULT_PERIOD="1y"
DEFAULT_STRATEGY="ire"

# Logging Configuration
LOG_LEVEL="INFO"
LOG_FILE="logs/online_trader.log"
